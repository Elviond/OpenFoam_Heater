FoamFile
{
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     foamRun;

solver          fluid;

startFrom       latestTime 

stopAt          endTime;

endTime         600;

deltaT          0.01;

writeControl    adjustableRunTime;

writeInterval   6;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

unTimeModifiable true;

adjustTimeStep  yes;

maxCo           4;


